{
  "name": "@turborepo/auth",
  "version": "1.0.0",
  "description": "Authentication SDK for Turborepo POC frontends",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
