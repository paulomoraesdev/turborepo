  },
  "devDependencies": {
    "@turborepo/eslint-config": "workspace:*",
    "@turborepo/typescript-config": "workspace:*",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "turborepo",
    "auth",
    "sdk",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.22.0"
